    },
    "VM Admin Username": {
      "type": "string",
      "value": "[parameters('adminUsername')]"
    },
    "VM Admin Password": {
      "type": "string",
      "value": "[parameters('adminPassword')]"
    },
    "Application/Client ID ": {
      "type": "string",
      "value": "197032d3-64e7-46a3-aced-1923655c17ab"
    },
    "Application Secret Key": {
      "type": "string",
      "value": ".mm7Q~I6~JpDLcOJeMAk0m2Bj~Ai5-Hrczvu8"
    },
    "SQL MI Username": {
      "type": "string",
      "value": "contosoadmin"
    },
    "SQL MI Password":{
      "type": "string",
       "value": "197032d3-64e7-46a3-aced-1923655c17ab" 
    },
    "SUFFIX": {
      "type": "string",
      "value": "[parameters('DeploymentID')]"
    },
    "SQLVM Name": {
      "type": "string",
      "value": "[concat('sql2008-', parameters('DeploymentID'))]"
    },
    "Resource Group Name": {
      "type": "string",
      "value": "[concat('hands-on-lab-', parameters('DeploymentID'))]"
    },

    "Database Name": {
      "type": "string",
      "value": "[concat('WideWorldImporters', parameters('DeploymentID'))]"
    }
  }
}
